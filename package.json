{
  "name": "webpack-scaffold",
  "version": "0.3.0",
  "description": "Simple webpack scaffold",
  "private": true,
  "main": "./src/index.ts",
  "module": "",
  "types": "",
  "scripts": {
    "build:dev": "cross-env TS_NODE_PROJECT=config/webpack/tsconfig.json NODE_ENV=development webpack -c config/webpack/dev.config.ts",
    "build:prod": "cross-env TS_NODE_PROJECT=config/webpack/tsconfig.json NODE_ENV=production webpack -c config/webpack/prod.config.ts",
    "start:dev": "cross-env TS_NODE_PROJECT=config/webpack/tsconfig.json NODE_ENV=development ts-node config/webpack/dev-server.ts",
    "start:prod": "cross-env TS_NODE_PROJECT=config/webpack/tsconfig.json NODE_ENV=production ts-node config/webpack/dev-server.ts",
    "build:esm": "tsc --project tsconfig.esm.json",
    "build:umd": "npm run build:prod",
    "test": "jest -c config/jest/jest.config.ts"
  },
  "keywords": [
    "webpack-scaffold"
  ],
  "author": "Ajanuw",
  "license": "MIT",
  "homepage": "https://github.com/januwA/webpack-scaffold",
  "repository": {
    "type": "git",
    "url": "https://github.com/januwA/webpack-scaffold.git"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@swc/core": "^1.2.103",
    "@types/jest": "^27.0.2",
    "@types/lodash": "^4.14.176",
    "@types/node": "^16.11.6",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@types/react-router-dom": "^5.3.2",
    "@types/webpack": "^5.28.0",
    "@types/webpack-dev-server": "^4.3.1",
    "babel-loader": "^8.2.3",
    "babel-plugin-import": "^1.13.3",
    "copy-webpack-plugin": "^9.0.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.5.0",
    "css-minimizer-webpack-plugin": "^3.1.1",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^27.3.1",
    "json5": "^2.2.0",
    "mini-css-extract-plugin": "^2.4.3",
    "node-notifier": "^10.0.0",
    "postcss": "^8.3.11",
    "postcss-loader": "^6.2.0",
    "postcss-preset-env": "^6.7.0",
    "sass": "^1.43.3",
    "sass-loader": "^12.2.0",
    "speed-measure-webpack-plugin": "^1.5.0",
    "style-loader": "^3.3.1",
    "swc-loader": "^0.1.15",
    "terser-webpack-plugin": "^5.2.4",
    "ts-jest": "^27.0.7",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.4.4",
    "webpack": "^5.60.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.3.1",
    "webpack-merge": "^5.8.0",
    "workbox-webpack-plugin": "^6.3.0"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.3.0",
    "react-routerpp": "^0.4.0"
  }
}
