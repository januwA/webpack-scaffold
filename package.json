{
  "name": "webpack-scaffold",
  "version": "0.3.0",
  "description": "Simple webpack scaffold",
  "private": true,
  "main": "./src/index.ts",
  "module": "",
  "types": "",
  "scripts": {
    "build:dev": "cross-env TS_NODE_PROJECT=config/webpack/tsconfig.json NODE_ENV=development webpack -c config/webpack/dev.config.ts",
    "build:prod": "cross-env TS_NODE_PROJECT=config/webpack/tsconfig.json NODE_ENV=production webpack -c config/webpack/prod.config.ts",
    "start:dev": "cross-env TS_NODE_PROJECT=config/webpack/tsconfig.json NODE_ENV=development ts-node config/webpack/dev-server.ts",
    "start:prod": "cross-env TS_NODE_PROJECT=config/webpack/tsconfig.json NODE_ENV=production ts-node config/webpack/dev-server.ts",
    "build:esm": "tsc --project tsconfig.esm.json",
    "build:umd": "npm run build:prod",
    "test": "jest -c config/jest/jest.config.ts"
  },
  "keywords": [
    "webpack-scaffold"
  ],
  "author": "Ajanuw",
  "license": "MIT",
  "homepage": "https://github.com/januwA/webpack-scaffold",
  "repository": {
    "type": "git",
    "url": "https://github.com/januwA/webpack-scaffold.git"
  },
  "devDependencies": {
    "@babel/core": "^7.22.19",
    "@babel/preset-env": "^7.22.15",
    "@swc/core": "^1.3.84",
    "@types/jest": "^29.5.4",
    "@types/lodash": "^4.14.198",
    "@types/node": "^20.6.0",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@types/react-router-dom": "^5.3.3",
    "@types/webpack": "^5.28.2",
    "@types/webpack-dev-server": "^4.7.1",
    "@types/webpack-env": "^1.18.1",
    "babel-loader": "^9.1.3",
    "babel-plugin-import": "^1.13.8",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.1",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "html-webpack-plugin": "^5.5.3",
    "jest": "^29.7.0",
    "json5": "^2.2.3",
    "mini-css-extract-plugin": "^2.7.6",
    "node-notifier": "^10.0.1",
    "postcss": "^8.4.29",
    "postcss-loader": "^7.3.3",
    "postcss-preset-env": "^9.1.3",
    "postcss-px-to-viewport": "^1.1.1",
    "sass": "^1.67.0",
    "sass-loader": "^13.3.2",
    "speed-measure-webpack-plugin": "^1.5.0",
    "style-loader": "^3.3.3",
    "swc-loader": "^0.2.3",
    "terser-webpack-plugin": "^5.3.9",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2",
    "webpack": "^5.88.2",
    "webpack-bundle-analyzer": "^4.9.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-merge": "^5.9.0",
    "workbox-webpack-plugin": "^7.0.0"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "path-browserify": "^1.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.16.0"
  }
}
