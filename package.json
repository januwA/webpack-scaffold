{
  "name": "webpack-scaffold",
  "version": "0.3.0",
  "description": "Simple webpack scaffold",
  "private": true,
  "main": "./src/index.ts",
  "module": "",
  "types": "",
  "scripts": {
    "build:dev": "cross-env TS_NODE_PROJECT=config/webpack/tsconfig.json NODE_ENV=development webpack -c config/webpack/dev.config.ts",
    "build:prod": "cross-env TS_NODE_PROJECT=config/webpack/tsconfig.json NODE_ENV=production webpack -c config/webpack/prod.config.ts",
    "start:dev": "cross-env TS_NODE_PROJECT=config/webpack/tsconfig.json NODE_ENV=development ts-node config/webpack/dev-server.ts",
    "start:prod": "cross-env TS_NODE_PROJECT=config/webpack/tsconfig.json NODE_ENV=production ts-node config/webpack/dev-server.ts",
    "build:esm": "tsc --project tsconfig.esm.json",
    "build:umd": "npm run build:prod",
    "test": "jest -c config/jest/jest.config.ts"
  },
  "keywords": [
    "webpack-scaffold"
  ],
  "author": "Ajanuw",
  "license": "MIT",
  "homepage": "https://github.com/januwA/webpack-scaffold",
  "repository": {
    "type": "git",
    "url": "https://github.com/januwA/webpack-scaffold.git"
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@babel/preset-env": "^7.20.2",
    "@swc/core": "^1.3.21",
    "@types/jest": "^29.2.4",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.11.11",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@types/react-router-dom": "^5.3.3",
    "@types/webpack": "^5.28.0",
    "@types/webpack-dev-server": "^4.7.1",
    "babel-loader": "^9.1.0",
    "babel-plugin-import": "^1.13.5",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.2",
    "css-minimizer-webpack-plugin": "^4.2.2",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^29.3.1",
    "json5": "^2.2.1",
    "mini-css-extract-plugin": "^2.7.2",
    "node-notifier": "^10.0.1",
    "postcss": "^8.4.19",
    "postcss-loader": "^7.0.2",
    "postcss-preset-env": "^7.8.3",
    "sass": "^1.56.1",
    "sass-loader": "^13.2.0",
    "speed-measure-webpack-plugin": "^1.5.0",
    "style-loader": "^3.3.1",
    "swc-loader": "^0.2.3",
    "terser-webpack-plugin": "^5.3.6",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.1",
    "typescript": "^4.9.3",
    "webpack": "^5.75.0",
    "webpack-bundle-analyzer": "^4.7.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1",
    "webpack-merge": "^5.8.0",
    "workbox-webpack-plugin": "^6.5.4"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.4.4"
  }
}